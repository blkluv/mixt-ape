{
  "name": "mixt-ape",
  "version": "0.1.0",
  "description": "Mixtapes for Solana.",
  "main": "index.js",
  "private": true,
  "author": "@h4rkl",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "cp .env.dev .env && next",
    "dev:prod": "cp .env.prod .env && next",
    "build": "next build && prisma generate",
    "start": "next start",
    "lint": "next lint",
    "meta:update": "ts-node scripts/set-meta.ts updateNFTs",
    "backup:prod": "bash scripts/backup-prod.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:setup": "rm -rf prisma/migrations && prisma migrate dev --name init && prisma generate",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "docker:build": "docker build . --platform=linux/amd64 -t mixtape/p1",
    "docker:push": "docker push mixtape/p1",
    "docker:deploy": "yarn docker:build && yarn docker:push",
    "docker:run": "docker run -p 3000:3000 mixtape",
    "deploy:prod": "bash scripts/deploy-prod.sh"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.11.17",
    "@distube/ytdl-core": "^4.13.2",
    "@dnd-kit/core": "^6.0.8",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@metaplex-foundation/js": "^0.19.4",
    "@metaplex-foundation/umi": "^0.8.9",
    "@metaplex-foundation/umi-bundle-defaults": "^0.8.7",
    "@metaplex-foundation/umi-signer-wallet-adapters": "^0.8.7",
    "@metaplex-foundation/umi-uploader-nft-storage": "^0.8.7",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.3.1",
    "@sentry/nextjs": "^7.67.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/wallet-adapter-base-ui": "^0.1.1",
    "@solana/wallet-adapter-react": "^0.15.34",
    "@solana/wallet-adapter-wallets": "^0.19.20",
    "@solana/web3.js": "^1.78.5",
    "@tiplink/api": "^0.1.3",
    "@vidstack/react": "^0.6.13",
    "dayjs": "^1.11.9",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "helius-sdk": "^1.0.14",
    "html-to-image": "^1.11.11",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "next": "^13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7",
    "sharp": "^0.32.6",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/mime-types": "^2.1.1",
    "@types/node": "20.4.9",
    "@types/react": "18.2.19",
    "commander": "^11.0.0",
    "prisma": "^5.3.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:h4rkl:mixt-ape.git"
  }
}
